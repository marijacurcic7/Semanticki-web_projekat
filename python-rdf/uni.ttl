            uni:points 10 ;
            uni:preExamination true ;
            uni:required true ],
        [ a uni:Evaluation ;
            uni:activity "Prisustvo na predavanjima" ;
            uni:points 5 ;
            uni:preExamination true ;
            uni:required true ],
        [ a uni:Evaluation ;
            uni:activity "Teorijski deo ispita" ;
            uni:points 70 ;
            uni:preExamination false ;
            uni:required true ],
        [ a uni:Evaluation ;
            uni:activity "Test" ;
            uni:points 10 ;
            uni:preExamination true ;
            uni:required true ] ;
    uni:hasLiterature [ a uni:Book ;
            dc:creator "Zedeck, S." ;
            dc:date 2011 ;
            dc:language "Engleski" ;
            dc:publisher "American Psychological Association" ;
            dc:title "APA Handbook of Industrial and Organizational Psychology" ],
        [ a uni:Book ;
            dc:creator "Guzina, M." ;
            dc:date 1966 ;
            dc:language "Srpski jezik" ;
            dc:publisher "Naučna knjiga, Beograd" ;
            dc:title "Kadrovska psihologija" ],
        [ a uni:Book ;
            dc:creator "Spector, P." ;
