@prefix : <http://www.semanticweb.org/marija/ontologies/2021/university#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://purl.org/vocab/aiiso/schema#> .
@prefix uni: <http://www.semanticweb.org/marija/ontologies/2021/university.owl> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

schema:name rdfs:domain schema:Programme,
        :Domain,
        :Test .

<http://www.ftn.uns.ac.rs/875535517/softversko-inzenjerstvo-i-informacione-tehnologije?period=z> a uni:Programme ;
    uni:EducationalField "Tehničko-tehnološke nauke" ;
    uni:LevelOfStudy "Osnovne akademske studije" ;
    uni:ScientificField "Elektrotehničko i računarsko inženjerstvo" ;
    uni:Title "Diplomirani inženjer softvera (Dipl.inž.softv.)" ;
    uni:espb 240 ;
    uni:name "softversko inzenjerstvo i informacione tehnologije" ;
    uni:semesters 8 .

<http://www.semanticweb.org/marija/ontologies/2021/university> a owl:Ontology ;
    owl:imports schema:,
        foaf: .

:DomainProblem a owl:Class ;
    rdfs:subClassOf :Domain .

:EducationalField a owl:Class ;
    rdfs:subClassOf schema:Programme .

:LevelOfStudy a owl:Class ;
    rdfs:subClassOf schema:Programme .

:Methodology a owl:Class ;
    rdfs:subClassOf schema:Course .

:Purpose a owl:Class ;
    rdfs:subClassOf schema:Course .

:Result a owl:Class ;
    rdfs:subClassOf schema:Course .

:ScientificField a owl:Class ;
    rdfs:subClassOf schema:Programme .

:Title a owl:Class ;
    rdfs:subClassOf schema:Programme .

:Year a owl:Class ;
    rdfs:subClassOf schema:Course .

:activity a owl:DatatypeProperty ;
    rdfs:domain :Evaluation ;
    rdfs:range xsd:string .

:correct a owl:DatatypeProperty ;
    rdfs:domain :Answer ;
    rdfs:range xsd:boolean .

:endDate a owl:DatatypeProperty ;
    rdfs:domain :Take ;
    rdfs:range xsd:dateTime .

:espb a owl:DatatypeProperty ;
    rdfs:domain schema:Course,
        schema:Programme ;
    rdfs:range xsd:integer .

:passed a owl:DatatypeProperty ;
    rdfs:domain :Take ;
    rdfs:range xsd:boolean .

:points a owl:DatatypeProperty ;
    rdfs:domain :Evaluation,
        :Question,
        :Take,
        :Test ;
    rdfs:range xsd:float .

:preExamination a owl:DatatypeProperty ;
    rdfs:domain :Evaluation ;
    rdfs:range xsd:boolean .

:required a owl:DatatypeProperty ;
    rdfs:domain :Evaluation ;
    rdfs:range xsd:boolean .

:semester a owl:DatatypeProperty ;
    rdfs:domain schema:Course ;
    rdfs:range xsd:integer .

:semesters a owl:DatatypeProperty ;
    rdfs:domain schema:Programme ;
    rdfs:range xsd:integer .

:startDate a owl:DatatypeProperty ;
    rdfs:domain :Take ;
    rdfs:range xsd:dateTime .

:typeOfTeaching a owl:DatatypeProperty ;
    rdfs:domain <http://www.w3.org/2000/10/swap/pim/contact#Person> ;
    rdfs:range xsd:string .

:Literature a owl:Class .

:Question a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass :Test ;
            owl:onProperty schema:part_of ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
            owl:onClass :Answer ;
            owl:onProperty :hasAnswer ] .

:createdBy a owl:ObjectProperty .

:domain a owl:ObjectProperty .

:doneBy a owl:ObjectProperty .

:hasAnswer a owl:ObjectProperty .

:hasLiterature a owl:ObjectProperty .

:of a owl:ObjectProperty .

:Answer a owl:Class .

:Domain a owl:Class .

<http://www.w3.org/2000/10/swap/pim/contact#Person> rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty schema:teaches ;
            owl:someValuesFrom schema:Course ] .

:Evaluation a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty schema:part_of ;
            owl:someValuesFrom schema:Course ] .

:Take a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass :Test ;
            owl:onProperty :of ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass <http://www.w3.org/2000/10/swap/pim/contact#Person> ;
            owl:onProperty :doneBy ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .

:Test a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass <http://www.w3.org/2000/10/swap/pim/contact#Person> ;
            owl:onProperty :createdBy ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass :Domain ;
            owl:onProperty :domain ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .

schema:Course rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty schema:part_of ;
            owl:someValuesFrom schema:Programme ],
        [ a owl:Restriction ;
            owl:onProperty :hasLiterature ;
            owl:someValuesFrom :Literature ] .

