:Answer a owl:Class .

:Domain a owl:Class .

<http://www.w3.org/2000/10/swap/pim/contact#Person> rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty schema:teaches ;
            owl:someValuesFrom schema:Course ] .

:Evaluation a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty schema:part_of ;
            owl:someValuesFrom schema:Course ] .

:Take a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass :Test ;
            owl:onProperty :of ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass <http://www.w3.org/2000/10/swap/pim/contact#Person> ;
            owl:onProperty :doneBy ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .

:Test a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass <http://www.w3.org/2000/10/swap/pim/contact#Person> ;
            owl:onProperty :createdBy ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass :Domain ;
            owl:onProperty :domain ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .

schema:Course rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty schema:part_of ;
            owl:someValuesFrom schema:Programme ],
        [ a owl:Restriction ;
            owl:onProperty :hasLiterature ;
            owl:someValuesFrom :Literature ] .

