:Literature a owl:Class .

:Question a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
            owl:onClass :Answer ;
            owl:onProperty :hasAnswer ],
        [ a owl:Restriction ;
            owl:onClass :Test ;
            owl:onProperty schema:part_of ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .

:createdBy a owl:ObjectProperty .

:domain a owl:ObjectProperty .

:doneBy a owl:ObjectProperty .

:hasAnswer a owl:ObjectProperty .

:hasLiterature a owl:ObjectProperty .

:of a owl:ObjectProperty .

