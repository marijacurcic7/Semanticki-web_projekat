###  http://www.semanticweb.org/marija/ontologies/2021/university#MyAnswer
:MyAnswer rdf:type owl:Class ;
          rdfs:subClassOf :Take .


###  http://www.semanticweb.org/marija/ontologies/2021/university#Question
:Question rdf:type owl:Class ;
          rdfs:subClassOf :Test .


###  http://www.semanticweb.org/marija/ontologies/2021/university#Take
:Take rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :hasAnswers ;
                        owl:someValuesFrom :MyAnswer
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :doneBy ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass <http://www.w3.org/2000/10/swap/pim/contact#Person>
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :partOf ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Test
                      ] .


###  http://www.semanticweb.org/marija/ontologies/2021/university#Test
:Test rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :hasQuestions ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Question
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :partOf ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass schema:Course
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :createdBy ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass <http://www.w3.org/2000/10/swap/pim/contact#Person>
                      ] .


[ owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
